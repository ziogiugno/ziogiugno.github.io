{
  "name": "mio-blog",
  "private": true,
  "description": "My blog powered by Gatsby and Markdown",
  "version": "0.1.0",
  "author": "Duma Rolando <ziogiugno@gmail.com>",
  "bugs": {
    "url": "https://github.com/ziogiugno/"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-minimal-blog": "^2.2.4",
    "@stefanprobst/lokijs": "^1.5.6-b",
    "gatsby": "^2.3.34",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "gh-pages": "^2.2.0",
    "prettier": "^1.17.0"
  },
  "homepage": "https://ziogiugno.github.io",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ziogiugno/ziogiugno.github.io.git"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "env-cmd -f .env.dev gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "build:gh": "npm run clean && npm run build && gh-pages -d public -b master",
    "clean": "rm -rf public && rm -rf .cache",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  }
}
